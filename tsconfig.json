{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "paths": {
      "@/*": [
        "./*"
      ],
      "@assets/*": [
        "./assets/*"
      ],
      "@components/*": [
        "./components/*"
      ],
      "@constants/*": [
        "./constants/*"
      ],
      "@contexts/*": [
        "./contexts/*"
      ],
      "@database/*": [
        "./database/*"
      ],
      "@features/*": [
        "./features/*"
      ],
      "@hooks/*": [
        "./hooks/*"
      ],
      "@i18n/*": [
        "./i18n/*"
      ],
      "@migrations/*": [
        "./migrations/*"
      ],
      "@plugins/*": [
        "./plugins/*"
      ],
      "@stores/*": [
        "./stores/*"
      ],
      "@utils/*": [
        "./utils/*"
      ]
    },
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "global.d.ts",
    "image.d.ts"
  ]
}